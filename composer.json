{
  "version": "4.0.0",
  "name": "everli/soldo-php-sdk",
  "description": "Unofficial PHP SDK to work with Soldo API",
  "license": "AGPL-3.0-only",
  "keywords": [
    "everli",
    "soldo",
    "api"
  ],
  "authors": [
    {
      "name": "Everli Team",
      "email": "tech@everli.com",
      "homepage": "https://everli.com"
    }
  ],
  "require": {
    "php": ">= 5.5",
    "guzzlehttp/guzzle": "~6.0",
    "psr/log": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.*",
    "symfony/var-dumper": "~3.3",
    "friendsofphp/php-cs-fixer": "~2.2"
  },
  "autoload": {
    "psr-4": {
      "Soldo\\": "src/Soldo/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Soldo\\Tests\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit test/"
  }
}
